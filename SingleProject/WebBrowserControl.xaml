<Window x:Class="SingleProject.WebBrowserControl"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="WebBrowserControl" Height="800" Width="800" Loaded="Window_Loaded">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="103*"/>
            <ColumnDefinition Width="689*"/>
        </Grid.ColumnDefinitions>
        <WebBrowser Name="myWeb" HorizontalAlignment="Left" Height="729" Margin="10,31,0,0" VerticalAlignment="Top" Width="772" Grid.ColumnSpan="2"/>
        <Button Content="Back" HorizontalAlignment="Left" Margin="10,4,0,0" VerticalAlignment="Top" Width="45" Height="22" Click="Button_Back"/>
        <Button Content="Forward" HorizontalAlignment="Left" Margin="60,4,0,0" VerticalAlignment="Top" Width="45" Height="22" Click="Button_Forward" Grid.ColumnSpan="2"/>
        <TextBox Name="address" HorizontalAlignment="Left" Height="23" Margin="7,4,0,0" TextWrapping="Wrap" Text="Enter Address" VerticalAlignment="Top" Width="637" Grid.Column="1"/>
        <Button Content="GO" HorizontalAlignment="Left" Margin="649,5,0,0" VerticalAlignment="Top" Width="30" Height="22" Click="Button_Go" Grid.Column="1"/>

    </Grid>
</Window>
